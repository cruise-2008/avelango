                                            <fieldset>
                                                <div class="form-group">
                                                    <label class="col-sm-3 control-label"><span translate="ORDERNAME"></span></label>
                                                    <div class="col-sm-9">
                                                        <input type="text" name="order_name" placeholder="placeholder" class="form-control" ng-model="task.order.name" required="" />
                                                        <span ng-class="{show:(task.orderForm.order_name.$dirty &amp;&amp; task.orderForm.order_name.$error.required)}" class="text-danger hide" translate="REQUIREDFIELD"></span>
                                                        <span ng-class="{show:(task.orderForm.order_name.$dirty &amp;&amp; task.orderForm.order_name.$error.validator)}" class="text-danger hide" translate="VALIDFIELD"></span>
                                                    </div>
                                                </div>
                                            </fieldset>
                                            <fieldset>
                                                <div class="form-group">
                                                    <label class="col-sm-3 control-label"><span translate="ORDERDESCRIPT"></span></label>
                                                    <div class="col-sm-9">
                                                        <textarea type="text" name="order_description" maxlength="500" placeholder="placeholder" class="form-control" value="" ng-model="task.order.description" required="" />
                                                        <span ng-class="{show:(task.orderForm.order_description.$dirty &amp;&amp; task.orderForm.order_description.$error.required)}" class="text-danger hide" translate="REQUIREDFIELD"></span>
                                                        <span ng-class="{show:(task.orderForm.order_description.$dirty &amp;&amp; task.orderForm.order_description.$error.validator)}" class="text-danger hide" translate="VALIDFIELD"></span>
                                                    </div>
                                                </div>
                                            </fieldset>
                                            <fieldset>
                                                <div class="form-group">
                                                    <label class="col-sm-3 control-label"><span translate="ORDERPRICE"></span></label>
                                                    <div class="col-sm-9">
                                                        <input type="text" name="order_price" placeholder="placeholder" class="form-control" ng-model="task.order.price" required="" ng-pattern="/^[\d]{1,12}$/" />
                                                        <span ng-class="{show:(task.orderForm.order_price.$dirty &amp;&amp; task.orderForm.order_price.$error.required)}" class="text-danger hide" translate="REQUIREDFIELD"></span>
                                                        <span ng-class="{show:(task.orderForm.order_price.$dirty &amp;&amp; task.orderForm.order_price.$error.pattern)}" class="text-danger hide" translate="VALIDNUMBER"></span>
                                                    </div>
                                                </div>
                                            </fieldset>
                                            <fieldset>
                                                <div class="form-group">
                                                    <label class="col-sm-3 control-label"><span translate="ORDERCOUNTRY"></span></label>
                                                    <div class="col-sm-9">
                                                        <select class="selectpicker" ng-model="task.country" ng-change="task.numberofloc(task.country)">
                                                            <option value="Задание актуально для страны" disabled translate=""></option>
                                                            <option ng-repeat="x in task.locations" value="{{x.Code}}" translate="{{x.Code}}"></option>
                                                        </select>
                                                    </div>
                                                </div>
                                            </fieldset>
                                            <fieldset>
                                                <div class="form-group">
                                                    <label class="col-sm-3 control-label"><span translate="ORDERCITY"></span></label>
                                                    <div class="col-sm-9">
                                                        <select class="selectpicker" ng-model="task.city">
                                                            <option value="Задание актуально для города" disabled translate=""></option>
                                                            <option ng-repeat="x in task.locations[task.count].Cities" value="{{x.Code}}" translate="{{x.Code}}"></option>
                                                        </select>
                                                    </div>
                                                </div>
                                            </fieldset>
                                            <fieldset>
                                                <div class="form-group">
                                                    <label class="col-sm-3 control-label" style="margin-top: 35px;"><span translate="ACTUALTIME"></span></label>
                                                    <div class="row" ng-controller="DatepickerDemoCtrl as dp">
                                                        <div class="col-md-5" style="margin-top: 30px;">
                                                            <p class="input-group">
                                                                <input type="text" name="order_data" uib-datepicker-popup="{{dp.format}}" ng-model="dp.dt" is-open="dp.opened" min-date="dp.minDate" datepicker-options="dp.dateOptions" date-disabled="dp.disabled(date, mode)" ng-required="" close-text="Close" class="form-control" />
                                                                <span class="input-group-btn">
                                                                    <button type="button" ng-click="dp.open($event)" class="btn btn-default">
                                                                        <em class="fa fa-calendar"></em>
                                                                    </button>
                                                                </span>
                                                                <span ng-class="{show:(task.orderForm.order_data.$dirty &amp;&amp; task.orderForm.order_data.$error.required)}" class="text-danger hide" translate="REQUIREDFIELD"></span>
                                                            </p>
                                                        </div>
                                                        <div class="col-md-3" ng-controller="TimepickerDemoCtrl as tp" id="timepick">
                                                            <uib-timepicker ng-model="tp.mytime" @*ng-change="tp.changed()"*@ hour-step="tp.hstep" minute-step="tp.mstep" show-meridian="tp.ismeridian"></uib-timepicker>
                                                            @*<pre class="alert alert-info"><span translate="TIME"></span>: {{tp.mytime | date:'shortTime' }}</pre>*@
                                                        </div>
                                                    </div>
                                                </div>
                                            </fieldset>
                                            <fieldset>
                                                <div class="form-group">
                                                    <input type="file" id="file-uploader" class="form-control" multiple>
                                                </div>
                                            </fieldset>
                                        </div>
                                        <div ng-class="{show:task.authMsg}" class="alert alert-danger text-center hide">{{task.authMsg}}</div>
                                        <div class="modal-footer"><button type="submit" class="btn btn-primary">OK</button><button type="button" ng-click="cancel()" class="btn btn-warning"><span translate="BTNCANCEL"></span></button></div>
                                    </form>
                                </div>
                            </div>
                        </script>
                    </div>
                </div>
            </div>
        </div>


    </section>





































